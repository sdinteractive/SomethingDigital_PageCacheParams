<?xml version="1.0"?>
<config>
    <global>
        <cache>
            <request_processors>
                <sd_pagecacheparams>SomethingDigital_PageCacheParams_Model_Processor</sd_pagecacheparams>
            </request_processors>
        </cache>

        <sd_pagecacheparams>
            <blacklist>
                <gclid />
                <gdftrk />
                <utm_content />
                <utm_term />
                <utm_campaign />
                <utm_medium />
                <utm_source />
                <_ga />
                <gclsrc />
                
                <!-- Mailchimp -->
                <mc_cid />
                <mc_eid />

                <!-- Bronto -->
                <_bta_tid />

                <!-- GoDataFeed -->
                <gdfms />
                <gdffi />
            </blacklist>
            
            <exclude_list>
                <listrak_remarketing path="/remarketing/cart/reload" />
            </exclude_list>
        </sd_pagecacheparams>
    </global>
</config>
